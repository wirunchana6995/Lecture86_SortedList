<Window x:Class="Lecture86_SortedList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lecture86_SortedList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="34" Margin="84,80,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="180"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="35" Margin="84,144,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="180"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="289,80,0,0" VerticalAlignment="Top" Width="214" Height="99" Click="Button_Click"/>

    </Grid>
</Window>
